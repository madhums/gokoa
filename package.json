{
  "name": "gokoa",
  "version": "0.0.1",
  "description": "A boilerplate for building web apps using koajs.",
  "main": "index.js",
  "engines": {
    "node": "0.11.x"
  },
  "scripts": {
    "test": "npm test",
    "start": "NODE_PATH=./app/controllers:./app/api:./config NODE_ENV=development ./node_modules/.bin/nodemon --harmony server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madhums/gokoa"
  },
  "keywords": [
    "koajs"
  ],
  "author": "Madhusudhan Srinivasa",
  "license": "MIT",
  "dependencies": {
    "koa": "latest",
    "koa-logger": "latest",
    "koa-router": "latest",
    "koa-static": "latest",
    "co-views": "latest",
    "co-body": "latest",
    "swig": "latest"
  },
  "devDependencies": {
    "nodemon": "latest"
  },
  "bugs": {
    "url": "https://github.com/madhums/gokoa"
  },
  "homepage": "https://github.com/madhums/gokoa"
}
